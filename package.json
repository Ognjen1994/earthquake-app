{
  "name": "earthquake-app",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "install:all": "pnpm install --filter backend && pnpm install --filter frontend",
    "dev": "concurrently \"pnpm --filter frontend dev\" \"pnpm --filter backend dev\"",
    "dev:backend": "pnpm --filter backend dev",
    "dev:frontend": "pnpm --filter frontend dev"
  },
  "workspaces": [
    "apps/*"
  ],
  "devDependencies": {
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0",
    "concurrently": "^8.0.0"
  },
  "packageManager": "pnpm@8.6.0"
}
